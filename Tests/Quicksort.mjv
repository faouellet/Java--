public class QuickSort
{
  static int n;
  static int[] m;

  static public void main()
  {
   rd();

   if ( n > 0 )
      {
       wr();
       printstr( "Sorting..." ); quicksort(1, n); println();
       wr();
      }
  }

  static void quicksort(int l, int u)
  {
   int i, j;
   int v, x;
   boolean loop;
   
   if ( u <= l ) return;

   i = l - 1;   j = u;   v = m[u];   loop = true;
   while (loop)
         {
          i = i + 1;
          while ( m[i] < v ) i = i + 1;
          j = j - 1;
          while ( m[j] > v ) j = j - 1;
          if ( i < j )
             {
              x = m[i];   m[i] = m[j];   m[j] = x;
             }
          else
             loop = false;
         }
   x = m[i];   m[i] = m[u];   m[u] = x;
   quicksort( l, j );
   quicksort( i+1, u );
   return;
  }

  static void rd()
  {
   int i;

   printstr( "Reading data set... " ); println();

   read( n );

   if ( n > 0 )
      {
       m = new int[n+2];

       i = 1;
       while ( i <= n )
             {
              read( m[i] );
              i = i + 1;
             }
      }
   print( n );   printstr( " items read. " ); println();
   m[0] = -1;    m[n+1] = 999999;
  }

  static void wr()
  {
   int i;

   printstr( "Writing data set... " ); println();

   i = 1;
   while ( i <= n )
         {
          print( m[i] ); println();
          i = i + 1;
         }
  }

}
